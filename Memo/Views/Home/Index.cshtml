@using Memo.Models;
@using System.Data;

@{
    ViewBag.Title = "Home Page";
}
<h2>Home</h2>




<table class="table table-responsive table-striped" >
    <thead>
        <tr>RNo</tr>
        <tr>RNo</tr>
        <tr>RNo</tr>
        <tr>RNo</tr>
        <tr>RNo</tr>
        <tr>RNo</tr>
        <tr>RNo</tr>
        <tr>RNo</tr>
    </thead>
    <tbody>
        @foreach (var d in Model)
        {
            <tr>
                <td>@d.EmployeeId</td>
                <td>@d.FirstName</td>
                <td>@d.LastName</td>
                <td>@d.Email</td>
                <td>@d.Salary</td>
                <td>@d.Company</td>
                <td>@d.Dept</td>

            </tr>
        }  
    </tbody>
</table>







